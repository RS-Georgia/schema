<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="rge" xmlns:rsi="rsi" targetNamespace="rge" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="rsi" schemaLocation=".././Level1/Level1Types.xsd"/>
	<xs:complexType name="B2BTp1GameSummaryByServiceProvider">
		<xs:sequence>
			<xs:element name="SmryGmAcctByAggrt" type="GameSummaryByAggregator" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="SmryGmAcctByRGS" type="GameSummaryByRemoteGamingSystem" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="B2CSummaryCashFlowType">
		<xs:sequence>
			<xs:element name="SmryPlyrAccntTrf" type="B2CSummaryPlayerAccountTransfersType"/>
			<xs:element name="SmryGmWgrTrf" type="GameSummary"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="B2CSummaryGameAccountingByGameVertical">
		<xs:sequence>
			<xs:element name="GameVertical" type="rsi:GameVerticalType">
				<xs:annotation>
					<xs:documentation>Game Sector such as Casino, Sportbetting, Live Casino and Poker</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GmSmry" type="GameSummary">
				<xs:annotation>
					<xs:documentation>Game Accounting Summary</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="B2CSummaryPlayerAccountTransfersType">
		<xs:sequence>
			<xs:element name="TtlDpsts" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount Deposited by Players</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlWthdrwls" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Withdrawals by Players</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BettingSummary">
		<xs:sequence>
			<xs:element name="TtlStakeAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Stake Amount in GEL of bets during period</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlPayOutAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total amount in GEl paid out   to players for betting wins during period</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlVoidedAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total amount in GEl of voided stakes and funds returned to players  (i.e. where not paid due to event cancellation, etc.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlStkPendingAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount in GEL currently pending stakes  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlNmbrBetSlipsSold" type="rsi:Meter">
				<xs:annotation>
					<xs:documentation>Total number of bet slips sold during the period.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlNmbrBetSlipsCancelled" type="rsi:Meter">
				<xs:annotation>
					<xs:documentation>Total number of Bet slips cancelled (ie. players got paid).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlNmbrBetSlipsVoided" type="rsi:Meter">
				<xs:annotation>
					<xs:documentation>Total number of bet slips voided (i.e. where not paid due to event cancellation, etc.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlNmbrBetsPlaced" type="rsi:Meter">
				<xs:annotation>
					<xs:documentation>Total number of bets placed in Georgia</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlNmbrP2PBetsPlaced" type="rsi:Meter" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total number of P2P bets placed in Georgia. Beting Exchange Only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlP2PStakeAmt" type="rsi:MonetaryAmountPositive" minOccurs="0"/>
			<xs:element name="TtlNmbrVirtualBets" type="rsi:Meter" minOccurs="0"/>
			<xs:element name="TtlVirtualBetsAmt" type="rsi:MonetaryAmountPositive" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BettingSummaryBySalesChannel">
		<xs:sequence>
			<xs:element name="SalesChannel" type="rsi:SalesChannelType">
				<xs:annotation>
					<xs:documentation>Betting Summary By Sales Vertical</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BettingAcctSmry" type="BettingSummary">
				<xs:annotation>
					<xs:documentation>Game Accounting Summary</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GameSummary">
		<xs:sequence>
			<xs:element name="TtlAmttoGm" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Totla Amount of Funds from Players to Game/s.(ie Stakes or Wagers)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlAmtfrmGm" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total amount of funds transfers from Game/s to Players. (i.e Wins to Player)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlAmtInPly" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount of funds currently pending in Game/s </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OperatingSummary">
		<xs:sequence>
			<xs:element name="TtlPlyrNmbr" type="rsi:Counter">
				<xs:annotation>
					<xs:documentation>Total Number of Players at the end of the month. Active players means players who actually have money in their wallets , whether these have played or not during the month. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlActvPlyrNmbr" type="rsi:Counter">
				<xs:annotation>
					<xs:documentation>Total Number of Players who where active during the month. Active players means players who have deposited, withdrawn, played or won during the month</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OpnBlncAmt" type="rsi:MonetaryAmount">
				<xs:annotation>
					<xs:documentation>Opening Balance Amount in Player Account the beginning of the month</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlDpstsAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount of Deposits made by player during month </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlWthdrwAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount of funds withdrawn by players during month</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlBetStkAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount of Betting stakes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlBetPytAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount of Payout made to players from betting duting month</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlBetVdAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total amount of Bets voided during month</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlGmWgrAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount of wagering on games excluding betting</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlGmWinningsAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount of winnings from games excluding betting</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlRkAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total amount of income generated from players during month to reporting operator for games such as poker and other P2P games</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlFreeWagersAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total Amount given in Free bets or wagers</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlTrnmtIncmAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total amount of income generated from players during the month from Tornaments</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlOvrlysAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total amount of overlays costs by operator for tournamet games such as poker tournamnts, slot tournaments, blackjack tournaments, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TtlJckptAmt" type="rsi:MonetaryAmountPositive">
				<xs:annotation>
					<xs:documentation>Total amount paid out in Jackpots</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClsBlncAmt" type="rsi:MonetaryAmount">
				<xs:annotation>
					<xs:documentation>Closing Balance Amount in Players Accounts for the month</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GameSummaryByAggregator">
		<xs:sequence>
			<xs:element name="AggrtID" type="rsi:Description">
				<xs:annotation>
					<xs:documentation>Aggregator ID as identified by the B2B platform</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GmAcctSmryByRGS" type="GameSummaryByRemoteGamingSystem" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Game Accounting Summary</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GameSummaryByB2COperatorByGame">
		<xs:sequence>
			<xs:element name="B2COprID" type="rsi:Max35Text">
				<xs:annotation>
					<xs:documentation>B2C Operator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GmAcctSmryByGmID" type="GameSummaryByGameID" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Game Accounting Summary by Game ID</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GameSummaryByGameID">
		<xs:sequence>
			<xs:element name="GameID" type="rsi:Description">
				<xs:annotation>
					<xs:documentation>Game ID as identified by the B2B platform</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GmAcctSmry" type="GameSummary">
				<xs:annotation>
					<xs:documentation>Game Accounting Summary</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GameSummaryByRemoteGamingSystem">
		<xs:sequence>
			<xs:element name="RmtGmSysID" type="rsi:Description">
				<xs:annotation>
					<xs:documentation>Remote Gaming System ID as identified by the B2B platform or Aggregator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GmAcctSmryByOprByGm" type="GameSummaryByB2COperatorByGame" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Game Accounting Summary</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
